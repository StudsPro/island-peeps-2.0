<meta http-equiv="X-UA-Compatible" content="IE=10,chrome=1" />
<meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1, user-scalable=no">
<meta name="charset" content="UTF-8" />
<meta name="csrf" content="{{csrf}}" />
<meta name="title" content="{{meta_title|default(title)}}" />
<meta name="description" content="{{meta_description|escape('html_attr')[:147] ~ '...'|default('')[:0]}}" />
<meta name="keywords" content="{{meta_keywords|default('stream mixtapes, download, hiphop, rap, artist name, mixtape, hip hop, hip-hop')|escape('html_attr')}}" />
<meta name="medium" content="audio" />
<meta name="cachedRender" content="{{cached|default('false')}}" />

{% if no_follow is defined %}
<meta name="robots" content="noindex, nofollow">
{% else %}
<meta name="robots" content="all" />
{% endif %}

<meta name="twitter:card" content="summary_large_image"/>
<meta name="twitter:description" content="{{meta_description|escape('html_attr')[:147] ~ '...'|default('')[:0]}}"/>
<meta name="twitter:title" content="{{meta_title|default(title)}}"/>
<meta name="twitter:site" content="@wixtape"/>
<meta name="twitter:domain" content="wixtape"/>
<meta name="twitter:image:src" content="{{meta_img|default(base_url ~ 'static/icons/apple-touch-icon-144.png')}}"/>
<meta property="og:site_name" content="WixTape" />
<meta property="og:url" content="{{url}}">
<meta property="og:title" content="{{meta_title|default(title)}}" />
<meta property="og:description" content="{{meta_description|escape('html_attr')[:147] ~ '...'|default('')[:0]}}" />
<meta property="og:image" content="{{meta_img|default(base_url ~ 'static/icons/apple-touch-icon-144.png')}}" />
<meta property="og:type" content="{{meta_type|default('website')}}" />