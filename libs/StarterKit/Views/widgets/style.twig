<link href='https://fonts.googleapis.com/css?family=Open+Sans:400,300italic,400italic,700italic,300,700' rel='stylesheet' type='text/css'>
<link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.4.0/css/font-awesome.min.css" rel="stylesheet" type="text/css" >
<link href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/3.4.0/animate.min.css" rel="stylesheet" type="text/css">
<link href="{{base_url}}static/adm/css/social1.core.css" rel="stylesheet" type="text/css">
<link href="{{base_url}}static/adm/css/social1.admin.css" rel="stylesheet" type="text/css"> 
<link href="{{base_url}}static/adm/css/themes/admin/facebook.css" rel="stylesheet" type="text/css">
<link href="{{base_url}}static/adm/css/stylecustom.css" rel="stylesheet" type="text/css" />
<link id="theme" rel="stylesheet" href="{{base_url}}static/adm/css/themes/social.theme-{{admin.theme|default('blue')}}.css" type="text/css">
<link href="{{base_url}}static/shared.css" rel="stylesheet" type="text/css">

{% if styles is iterable %}
	{%for href in styles %}
		<link rel="stylesheet" type="text/css" href="{{base_url}}static/adm/{{href}}" />
	{% endfor %}
{% else %}
	{% if styles is empty %}
		{{styles}}
	{% else %}
		<link rel="stylesheet" type="text/css" href="{{base_url}}static/adm/{{styles}}" />
	{% endif %}
{% endif %}
{% if styles_external is iterable %}
	{%for href in styles_external %}
		<link rel="stylesheet" type="text/css" href="{{href}}" />
	{% endfor %}
{% else %}
	{% if styles_external is empty %}
		{{styles_external}}
	{% else %}
		<link rel="stylesheet" type="text/css" href="{{styles_external}}" />
	{% endif %}
{% endif %}