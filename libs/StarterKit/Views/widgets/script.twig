<script src="//code.jquery.com/jquery-2.1.4.min.js"></script>
<script src="{{base_url}}static/plugin/noty/noty.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/remodal/1.0.2/remodal.min.js"></script>
<script src="{{base_url}}static/plugin/dropdown/dropdown.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-cookie/1.4.1/jquery.cookie.min.js"></script>
{% if minify %}
<script src="{{base_url}}static/script.min.js"></script>
{% else %}
<script src="{{base_url}}static/script.js"></script>
{% endif %}
{% if scripts_external is iterable %}
	{%for src in scripts_external %}
		<script type="text/javascript" src="{{src}}"></script>
	{% endfor %}
{% else %}
	{% if scripts_external is empty %}
		{{scripts_external}}
	{% else %}
		<script type="text/javascript" src="{{scripts_external}}"></script>
	{% endif %}
{% endif %}
{% if scripts is iterable %}
	{%for src in scripts %}
		<script type="text/javascript" src="{{base_url}}static/{{src}}"></script>
	{% endfor %}
{% else %}
	{% if scripts is empty %}
		{{scripts}}
	{% else %}
		<script type="text/javascript" src="{{base_url}}static/{{scripts}}"></script>
	{% endif %}
{% endif %}
{% if scripts_inline is defined %}
{{scripts_inline|raw}}
{%endif%}
