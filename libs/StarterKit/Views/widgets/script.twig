{% if scripts_external is iterable %}
	{%for src in scripts_external %}
		<script type="text/javascript" src="{{src}}"></script>
	{% endfor %}
{% else %}
	{% if scripts_external is empty %}
		{{scripts_external}}
	{% else %}
		<script type="text/javascript" src="{{scripts_external}}"></script>
	{% endif %}
{% endif %}
{% if scripts is iterable %}
	{%for src in scripts %}
		<script type="text/javascript" src="{{base_url}}static/adm/{{src}}"></script>
	{% endfor %}
{% else %}
	{% if scripts is empty %}
		{{scripts}}
	{% else %}
		<script type="text/javascript" src="{{base_url}}static/adm/{{scripts}}"></script>
	{% endif %}
{% endif %}
